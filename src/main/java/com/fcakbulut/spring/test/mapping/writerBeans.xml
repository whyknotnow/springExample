<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="logger" class="com.fcakbulut.spring.test.writer.Logger"
		p:consoleWriter-ref="consoleWriter" p:fileWriter-ref="fileWriter"></bean>

	<bean id="consoleWriter" class="com.fcakbulut.spring.test.writer.ConsoleWriter"></bean>

	<bean id="fileWriter" class="com.fcakbulut.spring.test.writer.FileWriter"></bean>
</beans>
